              </Assign.Value>
            </Assign>
            <ui:ForEachRow DataTable="[cover_head]" DisplayName="For each row" sap2010:WorkflowViewState.IdRef="ForEachRow_4">
              <ui:ForEachRow.Body>
                <ActivityAction x:TypeArguments="sd:DataRow">
